    });

    // Function to handle click events for each list
    function setupPaxList(id, category) {
        const listItems = document.querySelectorAll(`#${id} .num-pax_list_item`);

        listItems.forEach(item => {
            item.addEventListener('click', function () {
                // Remove 'selected' class from all items in this list
                listItems.forEach(i => i.classList.remove('selected'));

                // Add 'selected' class to the clicked item
                this.classList.add('selected');

                // Update output (optional)
                updateOutput();
            });
        });
    }
let adults
    let children
    let infants
    let prefClass
    // Update the output div with the selected values
    function updateOutput() {
        adults = document.querySelector('#adults .selected').innerText;
        children = document.querySelector('#children .selected').innerText;
        infants = document.querySelector('#infants .selected').innerText;
        prefClass = document.querySelector('#prefclass').selectedOptions[0].text;
        document.getElementById('selected_Traveller_class').innerText = `${adults}A,${children}C,${infants}I,${prefClass}`
    }

    // Add event listener for the class select dropdown
    document.getElementById('prefclass').addEventListener('change', function () {
        updateOutput();
    });

    // Setup each pax list
    setupPaxList('adults', 'Adults');
    setupPaxList('children', 'Children');
    setupPaxList('infants', 'Infants');

    // Initial update
    updateOutput();

    async function SearchFlight() {
        if ($("#SearchForm").valid()) {
            var ele = document.getElementById("SearchForm").elements;
            var fd = new FormData();
            var checkboxValues = {};
            let adultcount=adults
            let childcount=children
            let infantcount=infants
            let prefclass=prefClass
            for (let i = 0; i < ele.length; i++) {
                if (ele[i].type !== "button") {

                    // Handle radio buttons
                    if (ele[i].type === "radio" && ele[i].checked === true) {
                        fd.append(ele[i].name, ele[i].value);
                    }

                    // Handle checkboxes
                    if (ele[i].type === "checkbox" && ele[i].checked === true) {
                        // If multiple checkboxes share the same name, collect their values
                        if (checkboxValues[ele[i].name]) {
                            checkboxValues[ele[i].name].push(ele[i].value);
                        } else {
                            checkboxValues[ele[i].name] = [ele[i].value];
                        }
                    }

                    // Handle other input types (text, select, etc.)
                    if (ele[i].type !== "radio" && ele[i].type !== "checkbox") {
                        fd.append(ele[i].name, ele[i].value);
                    }
                }
            }

            for (let name in checkboxValues) {
                fd.append(name, checkboxValues[name].join(','));
            }
            fd.append("adultcount",adultcount)
            fd.append("childcount",childcount)
            fd.append("infantcount",infantcount)
            fd.append("prefclass",prefclass)
            let url = "/check";
            let response = await fetch(url, {
                method: "POST",
                body: fd  // Convert to JSON string
            });

        }

    }
</script>
