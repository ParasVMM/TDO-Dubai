<script>
    $(document).ready(function () {
        $('#mobile').intlTelInput({
            initialCountry: "in", // Default initial country
            separateDialCode: true, // Shows the dial code separately in the input
            // utilsScript: "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/11.0.4/js/utils.js" // Add utility functions
        });
    });

    function toggleForm() {
        var checkbox = document.getElementById("gst-checkbox");
        var form = document.getElementById("gst-form");

        if (checkbox.checked) {
            form.style.display = "block"; // Show the form
        } else {
            form.style.display = "none"; // Hide the form
        }
    }

    function ActiveButton() {
        var checkbox = document.getElementById("acceptTerms");
        var button = document.getElementById("ActiveButton");

        if (checkbox.checked) {
            button.disabled = false; // Enable the button
        } else {
            button.disabled = true; // Disable the button
        }
    }

    function startTimer(duration, display) {
        let timer = duration, minutes, seconds;
        setInterval(function () {
            minutes = parseInt(timer / 60, 10);
            seconds = parseInt(timer % 60, 10);

            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            display.textContent = minutes + ":" + seconds;

            if (--timer < 0) {
                timer = 0;
            }
        }, 1000);
    }

    window.onload = function () {
        let fifteenMinutes = 60 * 15,
            display = document.getElementById('timer');
        startTimer(fifteenMinutes, display);
    };

    function ConfirmBooking() {
        if ($("#details").valid()) {
            // If form is valid, show the modal
            $("#bookingModal").modal("show");
        }
    }

    function MealModal() {

        $("#mealModal").modal("show");
    }

    function SeatModal() {

        $("#SeatModal").modal("show");
    }

    // Select the parent container that holds the traveller elements
    document.querySelector('.travellers-info').addEventListener('click', function (event) {
        // Check if the clicked element has the class 'traveller-name'
        if (event.target.classList.contains('traveller-name')) {
            // Remove 'active-traveller' from all traveller divs
            document.querySelectorAll('.traveller-name').forEach(function (traveller) {
                traveller.classList.remove('active-traveller');
            });

            // Add 'active-traveller' to the clicked traveller div
            event.target.classList.add('active-traveller');
        }
    });


    document.getElementById("seat").addEventListener("click", function () {
        var overlay = document.getElementById("seat-info-tooltip");
        // Toggle the visibility of the overlay
        overlay.style.display = (overlay.style.display === "none" || overlay.style.display === "") ? "block" : "none";
    });
    document.getElementById("seat1").addEventListener("click", function () {
        var overlay = document.getElementById("seat-info-tooltip");
        // Toggle the visibility of the overlay
        overlay.style.display = (overlay.style.display === "none" || overlay.style.display === "") ? "block" : "none";
    });
</script>
