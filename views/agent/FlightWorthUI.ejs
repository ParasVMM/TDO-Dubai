    async function ModifySearch() {
        if ($("#ModifyForm").valid()) {
            var ele = document.getElementById("ModifyForm").elements;
            var fd = new FormData();
            var checkboxValues = {};
            let adultcount=adults
            let childcount=children
            let infantcount=infants
            let prefclass=prefClass
            for (let i = 0; i < ele.length; i++) {
                if (ele[i].type !== "button") {

                    // Handle radio buttons
                    if (ele[i].type === "radio" && ele[i].checked === true) {
                        fd.append(ele[i].name, ele[i].value);
                    }

                    // Handle checkboxes
                    if (ele[i].type === "checkbox" && ele[i].checked === true) {
                        // If multiple checkboxes share the same name, collect their values
                        if (checkboxValues[ele[i].name]) {
                            checkboxValues[ele[i].name].push(ele[i].value);
                        } else {
                            checkboxValues[ele[i].name] = [ele[i].value];
                        }
                    }

                    // Handle other input types (text, select, etc.)
                    if (ele[i].type !== "radio" && ele[i].type !== "checkbox") {
                        fd.append(ele[i].name, ele[i].value);
                    }
                }
            }

            for (let name in checkboxValues) {
                fd.append(name, checkboxValues[name].join(','));
            }
            fd.append("adultcount",adultcount)
            fd.append("childcount",childcount)
            fd.append("infantcount",infantcount)
            fd.append("prefclass",prefclass)
            let url = "/check";
            let response = await fetch(url, {
                method: "POST",
                body: fd  // Convert to JSON string
            });

        }

    }
