    const thumb1 = document.getElementById('thumb1');
    const thumbWidth = thumb0.offsetWidth; // Assuming both thumbs have the same width
    const sliderWidth = slider.offsetWidth - thumbWidth; // Adjust for thumb width
    let isDragging = null;

    const moveThumb = (e) => {
        if (!isDragging) return;

        const thumb = isDragging;
        let newLeft = e.clientX - slider.getBoundingClientRect().left;

        // Constrain thumb0's movement (it can't go beyond thumb1's position)
        if (thumb === thumb0) {
            const thumb1Position = parseInt(thumb1.style.left, 10) || sliderWidth;
            if (newLeft < 0) newLeft = 0;
            if (newLeft > thumb1Position - thumbWidth) newLeft = thumb1Position - thumbWidth;
        }

        // Constrain thumb1's movement (it can't go behind thumb0's position)
        if (thumb === thumb1) {
            const thumb0Position = parseInt(thumb0.style.left, 10) || 0;
            if (newLeft > sliderWidth) newLeft = sliderWidth;
            if (newLeft < thumb0Position + thumbWidth) newLeft = thumb0Position + thumbWidth;
        }

        // Update the thumb's position
        thumb.style.left = `${newLeft}px`;
    };

    const stopDragging = () => {
        isDragging = null;
        document.removeEventListener('mousemove', moveThumb);
        document.removeEventListener('mouseup', stopDragging);
    };

    const startDragging = (e, thumb) => {
        isDragging = thumb;
        document.addEventListener('mousemove', moveThumb);
        document.addEventListener('mouseup', stopDragging);
    };

    // Attach mouse down event to each thumb
    thumb0.addEventListener('mousedown', (e) => startDragging(e, thumb0));
    thumb1.addEventListener('mousedown', (e) => startDragging(e, thumb1));
});

