                            checkboxValues[ele[i].name] = [ele[i].value];
                        }
                    }

                    // Handle other input types (text, select, etc.)
                    if (ele[i].type !== "radio" && ele[i].type !== "checkbox") {
                        fd.append(ele[i].name, ele[i].value);
                    }
                }
            }

            for (let name in checkboxValues) {
                fd.append(name, checkboxValues[name].join(','));
            }
            fd.append("adultcount",adultcount)
            fd.append("childcount",childcount)
            fd.append("infantcount",infantcount)
            let url = "/check";
