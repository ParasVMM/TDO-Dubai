    // Function to handle step transitions
    function goToNextStep(stepNumber) {
        if (stepNumber >= 1 && stepNumber < 3) {
            $(".step-" + stepNumber).show();
            currentStep = stepNumber;

            if (currentStep < 3) {

                // Animate current step out
                $(".step-" + currentStep).addClass("animate__animated animate__fadeOutLeft");
                currentStep++;

                setTimeout(function () {
                    // Hide all steps and remove animations
                    $(".step").removeClass("animate__animated animate__fadeOutLeft").hide();
                    // Show the next step with animation
                    $(".step-" + currentStep).show().addClass("animate__animated animate__fadeInRight");
                    updateProgressBar();
                }, 500);
            }
        }
    }

    // Function to add 6 months to the current date and return in YYYY-MM-DD format
    function getMinDateSixMonthsFromToday() {
        var today = new Date();
        today.setMonth(today.getMonth() + 6);
        return today.toISOString().split('T')[0];
    }

    // Get the date 6 months from today
    const minDate = getMinDateSixMonthsFromToday();

    // Set the min attribute to the date 6 months from today
    document.getElementById('PassportExpiryDate1').setAttribute('min', minDate);
    document.getElementById('PassportExpiryDate2').setAttribute('min', minDate);
    document.getElementById('EmiratesIdValidity1').setAttribute('min', minDate);
    document.getElementById('EmiratesIdValidity2').setAttribute('min', minDate);
    document.getElementById('newPassportExpiryDate').setAttribute('min', minDate);
    document.getElementById('newEmiratesIdValidity').setAttribute('min', minDate);


