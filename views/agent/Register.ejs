    function validateAccountNumber() {
        var accountNumberInput = document.getElementById('accountNumber');
        var accountNumberError = document.getElementById('accountNumberError');

        if (accountNumberInput.value.length < 12) {
            event.preventDefault();  // Prevent form submission
            return accountNumberError.textContent = "Account number must be 12 digits long.";
        } else {
            accountNumberError.textContent = ""; // Clear error message if valid
        }
    }

    function validateTradeLicense() {
        var tradeLicenseId = document.getElementById('tradeLicense').value;
        var errorDiv = document.getElementById('tradeLicenseError');

        // Check if the input is numeric and has exactly 10 digits
        var isNumeric = /^\d{10}$/.test(tradeLicenseId);

        if (tradeLicenseId.length > 0 && !isNumeric) {
            errorDiv.textContent = 'Trade License Number must be exactly 10 digits long and numeric.';
        } else {
            errorDiv.textContent = ''; // Clear the error message when valid
        }
    }


    function validatePassportNumber(input) {
        // Replace any non-uppercase letters or digits
        input.value = input.value.replace(/[^A-Z0-9]/g, '');

        // Trim the value if it's more than 9 characters
        if (input.value.length > 9) {
            input.value = input.value.slice(0, 9);
        }
    }

