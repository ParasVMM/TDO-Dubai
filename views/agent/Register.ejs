<script>

    async function getStaff_id() {
        try {
            const staff_id = document.getElementById("staffId").value;
            console.log(staff_id);

            let url = "/Verify-Staff/" + staff_id;
            console.log(url);

            let response = await fetch(url, {
                headers: { "Content-Type": "application/json" }
            });

            response = await response.json();
            console.log(response);

            if (response.warning) {
                Swal.fire({
                    title: "OOPS!",
                    text: response.message,
                    icon: "warning"
                });
            } else if (response.message === 'success') {
                // Disable the input fields after success
                document.getElementById("staffId").disabled = true;
                document.getElementById("verify_staff_id").disabled = true;

                // Retrieve and assign staff name
                const staff_n = response.data[0];
                console.log(staff_n);

                const staff_name = staff_n['first_name'] + ' ' + staff_n['last_name'];
                document.getElementById("staffName").value = staff_name;
            }

        } catch (e) {
            // Handle any errors during the fetch call or data processing
            Swal.fire({
                title: "OOPS!",
                text: response.message,
                icon: "error"
            });
            console.error("Error:", e);
        }
    }


    function showButton() {
