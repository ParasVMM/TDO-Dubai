        }
    }

    function validateMobileNumberIndia(mobile, errorId) {

        const mobileInput = mobile;
        const mobileError = errorId;
        // console.log(mobileInput, mobileError)

        // Regular expression for Indian mobile numbers
        const regex = /^[6-9][0-9]{9}$/;

        // Test the input value against the regular expression
        if (regex.test(mobileInput)) {
            document.getElementById(mobileError).style.display = 'none';
            document.getElementById(mobileError).classList.remove('is-invalid');
            document.getElementById(mobileError).classList.add('is-valid');

            return true;
        } else {
            document.getElementById(mobileError).style.display = 'block';
            document.getElementById(mobileError).classList.remove('is-valid');
            document.getElementById(mobileError).classList.add('is-invalid');

            return false;
        }
    }


