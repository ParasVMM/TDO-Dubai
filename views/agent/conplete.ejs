        }
    }

    // Function to handle step transitions
    function goToNextStep(stepNumber) {
        if (stepNumber >= 1 && stepNumber < 3) {
            $(".step-" + stepNumber).show();
            currentStep = stepNumber;

            if (currentStep < 3) {
                // Animate current step out
                $(".step-" + currentStep).addClass("animate__animated animate__fadeOutLeft");
                currentStep++;

                setTimeout(function () {
                    // Hide all steps and remove animations
                    $(".step").removeClass("animate__animated animate__fadeOutLeft").hide();
                    // Show the next step with animation
                    $(".step-" + currentStep).show().addClass("animate__animated animate__fadeInRight");
                    updateProgressBar();
                }, 500);
            }
        }
    }

    // Get today's date in the format YYYY-MM-DD
    const today = new Date().toISOString().split('T')[0];

    // Set the min attribute to today's date
    document.getElementById('PassportExpiryDate1').setAttribute('min', today);
    document.getElementById('PassportExpiryDate2').setAttribute('min', today);
    document.getElementById('EmiratesIdValidity1').setAttribute('min', today);
    document.getElementById('EmiratesIdValidity2').setAttribute('min', today);
    document.getElementById('newPassportExpiryDate').setAttribute('min', today);
    document.getElementById('newEmiratesIdValidity').setAttribute('min', today);


    $(document).ready(function () {
        $('#multi-step-form').find('.step').slice(1).hide();


        $(".prev-step").click(function () {
            if (currentStep > 1 && currentStep <= 3) {
                $(".step-" + currentStep).addClass("animate__animated animate__fadeOutRight");
                currentStep--;
                setTimeout(function () {
                    $(".step").removeClass("animate__animated animate__fadeOutRight").hide();
                    $(".step-" + currentStep).show().addClass("animate__animated animate__fadeInLeft");
                    updateProgressBar();
                }, 500);
            }
        });

        updateProgressBar = function () {
            var progressPercentage = ((currentStep - 1) / 2) * 100;
            $(".progress-bar").css("width", progressPercentage + "%");
        }
    });


</script>

