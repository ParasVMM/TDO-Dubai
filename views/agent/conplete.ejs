                document.getElementById("submitButton").disabled = true;
                // let ip = 'http://192.168.1.30:4000'
                // let url = ip + "/add_agents";
                let url = "/add_agents";
                let response = await fetch(url, {
                    method: "POST",
                    body: formData  // Convert to JSON string
                });
                let result = await response.json();
                console.log("rtghjj", result)
                if (result.error) {
                    Swal.fire({
                        title: "OOPS!",
                        text: result.message,
                        icon: "question"
                    });
                    document.getElementById("submitButton").disabled = false;
                } else {
                    Swal.fire({
                        title: "Success",
