<script>
    async function modifySearch() {
        console.log("In Modify Search");
        const savedData = sessionStorage.getItem("fd");

        if (savedData) {
            const searchData = JSON.parse(savedData); // Parsing savedData into an object
            const cityName = searchData.cityName;
            console.log("Search Data", searchData);

            // Prefill destination, check-in, and check-out dates
            document.getElementById("destination").value = cityName || '';
            document.getElementById("destinationSelect").value = searchData.LocationID;
            document.getElementById("checkIn").value = searchData.checkIn || '';
            document.getElementById("checkOut").value = searchData.checkOut|| '';
            document.getElementById("checkOut").min = searchData.checkIn;
            document.getElementById("country").value = searchData.country;
            document.getElementById("correlationId").value = searchData.correlationId;

            // Access searchQuery from searchData
            if (searchData && searchData.rooms) {
                // Set the number of rooms in the dropdown
                const numberOfRooms = searchData.rooms.length;
                document.getElementById("roomsDropdown").value = numberOfRooms;

                let roomList;
                try {
                    roomList = typeof searchData.rooms === "string" ? JSON.parse(searchData.rooms) : (Array.isArray(searchData.rooms) ? searchData.rooms : []);
                } catch (error) {
                    return Swal.fire({
                        icon: error,
                        title: "Invalid format for 'rooms'. Ensure it's a valid JSON array."
                    });
                }

                // Check if rooms array is empty
                if (!Array.isArray(roomList) || roomList.length === 0) {
                    return res.status(400).json({
                        error: true,
                        message: "Rooms data is required and should be a non-empty array."
                    });
                }

                // Prepare occupancies from rooms
                const occupancies = roomList.map(room => ({
                    numberOfAdults: room.adults || 0,
                    numberOfChild: room.childrenAges.length || 0,
                    childAge : room.childrenAges || []
                }));
                // Trigger the function to generate room sections based on the number of rooms
                generateRooms(numberOfRooms);
                console.log(occupancies)
                // Loop through each room and set values for adults, children, and child ages
                occupancies.forEach((room, index) => {
                    // Select the current room section by its index
                    const roomSection = document.getElementById(`room-${index + 1}`);
                    if (roomSection) {
                        // Set the number of adults
                        const adultsDropdown = document.getElementById(`adultsDropdown${index}`);
                        if (adultsDropdown) {
                            adultsDropdown.value = room.numberOfAdults || 0;
                        }

                        // Set the number of children
                        const childrenDropdown = document.getElementById(`childrenDropdown${index}`);
                        if (childrenDropdown) {
                            childrenDropdown.value = room.numberOfChild || 0;
                            generateChildrenAges(index); // Generate child age dropdowns for the room
                        }

                        // Prefill ages for each child if applicable
                        if (room.numberOfChild > 0 && room.childAge) {
                            room.childAge.forEach((age, childIndex) => {
                                const childAgeDropdown = document.getElementById(`childAgeDropdown${index}_${childIndex}`);
                                if (childAgeDropdown) {
                                    childAgeDropdown.value = age || 0;
                                }
                            });
                        }
                    }
                });
            }
        }
    }

    function generateRooms(numberOfRooms) {
        const roomsContainer = document.getElementById('roomsContainer');
        roomsContainer.style.display = 'block'; // Ensure the container is visible
        roomsContainer.innerHTML = ''; // Clear previous room sections

        for (let i = 0; i < numberOfRooms; i++) {
            const roomSection = document.createElement('div');
            roomSection.id = `room-${i + 1}`; // Set unique ID for the room section
            roomSection.className = 'room-section';
            roomSection.innerHTML = `
        <div class="room-header-inline" style="margin-top: 10px;">
            <span style="display: block"><b>Room ${i + 1}</b></span>
            <div class="form-group-inline" style="display: flex; gap: 20px;"> <!-- Flex container for dropdowns -->
                <div class="form-group">
                    <label for="adultsDropdown${i}" class="fw-bold" style="font-size:16px">Adults <small>(18+ Yrs)</small></label>
                    <select id="adultsDropdown${i}" class="adultsDropdown" style="height: 35px; padding-left: 15px;width: 90%;" onchange="updateSummary()">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="childrenDropdown${i}" style="font-size:16px" class="fw-bold">Children <small>(0-17 Yrs.)</small>:</label>
                    <select id="childrenDropdown${i}" class="childrenDropdown" style="height: 35px; padding-left: 15px;width: 90%;" onchange="generateChildrenAges(${i}); updateSummary()">
                        <option value="0">0</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                    </select>
                </div>
            </div>
        </div>
        <div id="childrenSection${i}" class="room-sub-section"></div>
        `;
            roomsContainer.appendChild(roomSection);
        }



        updateSummary(); // Update summary after generating the rooms
    }

    function hideRoomsSection() {
        const roomsContainer = document.getElementById('roomsContainer');
        roomsContainer.style.display = 'none';
    }

    function updateSummary() {
        // Get the number of rooms selected
        const numberOfRoomsDropdown = document.getElementById('roomsDropdown');
        if (!numberOfRoomsDropdown) {
            console.error('Rooms dropdown not found!');
            return; // Exit the function if the rooms dropdown does not exist
        }

        const numberOfRooms = parseInt(numberOfRoomsDropdown.value);
        let totalPersons = 0;

        for (let i = 0; i < numberOfRooms; i++) {
            // Find the adults and children dropdowns by their IDs
            const adultsDropdown = document.getElementById(`adultsDropdown${i}`);
            const childrenDropdown = document.getElementById(`childrenDropdown${i}`);

            // Log for debugging: Check if dropdowns exist
            if (!adultsDropdown) {
                console.warn(`Adults dropdown not found for index ${i}`);
            }
            if (!childrenDropdown) {
                console.warn(`Children dropdown not found for index ${i}`);
            }

            // Check if dropdowns exist before accessing their values
            const adults = adultsDropdown ? parseInt(adultsDropdown.value) : 0;
            const children = childrenDropdown ? parseInt(childrenDropdown.value) : 0;

            // Add adults and children to the total count
            totalPersons += adults + children;
        }

        // Update the summary text
        const summaryText = `<span><b>Rooms:</b></span> <b>${numberOfRooms}</b>, <span><b>Guests:</b></span> <b>${totalPersons}</b>`;
        const roomsSummaryElement = document.getElementById('roomsSummary');
        if (roomsSummaryElement) {
            roomsSummaryElement.innerHTML = summaryText;
        } else {
            console.error('Rooms summary element not found!');
        }
    }

    // Event listener for the rooms dropdown to handle changes
    document.getElementById('roomsDropdown').addEventListener('change', function () {
        const numberOfRooms = parseInt(this.value);
        generateRooms(numberOfRooms); // Generate rooms based on the new dropdown value
        updateSummary(); // Update summary after generating rooms
    });

    function generateChildrenAges(roomIndex) {
        const childrenSection = document.getElementById(`childrenSection${roomIndex}`);
        childrenSection.innerHTML = ''; // Clear previous child age dropdowns

        const numberOfChildren = document.getElementById(`childrenDropdown${roomIndex}`).value;

        const childrenContainer = document.createElement('div');
        childrenContainer.className = 'children-container';
        childrenContainer.style.display = 'flex'; // Use flexbox for inline display
        childrenContainer.style.gap = '20px'; // Add space between age dropdowns

        for (let i = 0; i < numberOfChildren; i++) {
            const childAgeDropdown = document.createElement('div');
            childAgeDropdown.className = 'form-group child-age-dropdown';
            childAgeDropdown.innerHTML = `
        <label style="margin-top: 5px" for="childAgeDropdown${roomIndex}_${i}">Age: ${i + 1}</label>
        <br>
        <select id="childAgeDropdown${roomIndex}_${i}" style="height: 35px; padding-left: 15px" class="child-age-select">
            ${Array.from({length: 17}, (_, age) => `<option value="${age + 1}">${age + 1}</option>`).join('')}
        </select>
    `;
            childrenContainer.appendChild(childAgeDropdown);
        }


        childrenSection.appendChild(childrenContainer);
        updateSummary();
    }

</script>


