    let allHotels = []; // This will hold the original hotel data
    let filteredHotels = []; // This will hold the filtered hotel data

    function filterHotels() {
        const selectedRatings = Array.from(document.querySelectorAll(".rating-checkbox:checked"))
            .map(checkbox => parseInt(checkbox.getAttribute("data-rating")));

        const selectedPropertyTypes = Array.from(document.querySelectorAll(".property-type-checkbox:checked"))
            .map(checkbox => checkbox.getAttribute("data-type"));

        const hotelNameSearch = document.getElementById("hotelNameSearch").value.toLowerCase();

        const selectedMealTypes = Array.from(document.querySelectorAll(".meal-type-checkbox:checked"))
            .map(checkbox => checkbox.getAttribute("data-meal"));

        // Parse and clean price range values
        let minPrice = parseFloat(document.getElementById("rangeSliderExample3MinResult").innerText.replace(/[^\d.]/g, ''));
        let maxPrice = parseFloat(document.getElementById("rangeSliderExample3MaxResult").innerText.replace(/[^\d.]/g, ''));

        console.log("Hotels before filtering:", allHotels);
        console.log("Selected Ratings:", selectedRatings);
        console.log("Selected Property Types:", selectedPropertyTypes);
        console.log("Hotel Name Search:", hotelNameSearch);
        console.log("Price Range:", minPrice, "-", maxPrice);

        filteredHotels = allHotels.filter(hotel => {
            const starRating = hotel.staticContent?.starRating ?? 0;
            const category = hotel.staticContent?.type ?? "";
            const hotelName = hotel.staticContent?.name?.toLowerCase() ?? "";
            const totalRate = hotel.rate?.Rateinfo?.amount_afterMarkup ?? Infinity; // Default to Infinity if missing

            const mealOptions = hotel.options || {}; // Ensure no undefined access

            const matchesMeal = selectedMealTypes.length === 0 || selectedMealTypes.some(meal => mealOptions[meal] === true);
            const matchesRating = selectedRatings.length === 0 || selectedRatings.includes(Number(starRating));
            const matchesPropertyType = selectedPropertyTypes.length === 0 || selectedPropertyTypes.includes(category);
            const matchesName = hotelNameSearch === "" || hotelName.includes(hotelNameSearch);
            const matchesPrice = totalRate >= minPrice && totalRate <= maxPrice;

            return matchesRating && matchesPropertyType && matchesName && matchesPrice && matchesMeal;
        });

        console.log("Hotels after filtering:", filteredHotels);

        document.getElementById("totalSearchResults").innerHTML = filteredHotels.length;
        displayItemsForPage(filteredHotels);
    }

