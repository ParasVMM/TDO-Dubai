    $(document).ready(function () {
        let debounceTimeout;

        $("#destination").on("input", function () {
            let query = $(this).val().trim();
            if (query.length < 1) {
                $("#autocomplete-results").hide(); // Hide if input is empty
                return;
            }

            clearTimeout(debounceTimeout);
            debounceTimeout = setTimeout(() => {
                fetch('/hotels/hotelSearchPlaces', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ q: query })
                })
                    .then(response => response.json())
                    .then(data => {
                        let locationSuggestions = data.data.locationSuggestions || [];
                        document.getElementById('correlationId').value = data.correlationId;

                        if (locationSuggestions.length === 0) {
                            $("#autocomplete-results").hide();
                            return;
                        }

                        // Prioritize: Hotels > Cities > Others
                        locationSuggestions.sort((a, b) => {
                            if (a.type === "City") return -1;
                            if (b.type === "City") return 1;
                            if (a.type === "Hotel") return -1;
                            if (b.type === "Hotel") return 1;
                            return 0;
                        });

                        let results = locationSuggestions.map(places =>
                            `<li class="dropdown-item" data-value="${places.id}" data-country="${places.country}" title="${places.fullName} (${places.type})">
                        <span class="place-name">${places.fullName}</span><br>
                        <span class="place-type">(${places.type})</span>
                    </li>`
                        ).join("");

                        $("#autocomplete-results").html(results).css("display", "block");
                    })
                    .catch(error => {
                        console.error("API Error:", error);
                        $("#autocomplete-results").hide();
                    });
            }, 300);
        });

        // Select an item from the list
        $(document).on("click", "#autocomplete-results li", function () {
            let selectedText = $(this).find(".place-name").text();
            let selectedId = $(this).attr("data-value");
            let selectedCountry = $(this).attr("data-country");

            $("#destination").val(selectedText);
            $("#destinationSelect").val(selectedId);
            $("#country").val(selectedCountry);
            $("#autocomplete-results").hide(); // Hide after selection
        });

        // Hide dropdown when clicking outside
        $(document).on("click", function (e) {
            if (!$(e.target).closest("#destination, #autocomplete-results").length) {
                $("#autocomplete-results").hide();
            }
        });

        // Initially hide the dropdown
        $("#autocomplete-results").hide();
    });

    let fetchDtVal = (checkInDate) => {
        // Convert the input date string into a Date object
        const checkInDateObj = new Date(checkInDate);

        // Get the next day's date
        checkInDateObj.setDate(checkInDateObj.getDate() + 1);

        // Format the date to YYYY-MM-DD format for the input field
        const year = checkInDateObj.getFullYear();
        const month = String(checkInDateObj.getMonth() + 1).padStart(2, '0'); // Months are 0-indexed
        const day = String(checkInDateObj.getDate()).padStart(2, '0');

        // Set the formatted date to the checkOut input field
        document.getElementById("checkOut").value = `${year}-${month}-${day}`;
    }

    // Get today's date in the format YYYY-MM-DD
    const today = new Date().toISOString().split('T')[0];

    // Set the min attribute of the start date to today's date
    const startDateInput = document.getElementById('checkIn');
    startDateInput.setAttribute('min', today);

    // Reference to the end date input
    const endDateInput = document.getElementById('checkOut');

    // Function to add one day to a given date
    function addOneDay(date) {
        const result = new Date(date);
        result.setDate(result.getDate() + 1);
        return result.toISOString().split('T')[0];
    }

    // Add an event listener to the start date input to update the end date input
    startDateInput.addEventListener('change', function () {
        const startDate = this.value;
        const minEndDate = addOneDay(startDate);
        endDateInput.setAttribute('min', minEndDate);
        endDateInput.value = minEndDate; // This sets the initial value to the calculated minimum end date
    });

    function openDtPicker(id) {
        document.getElementById(id).addEventListener('click', function () {
            this.showPicker();
            // alert(id.value)
        });
    }


</script>


