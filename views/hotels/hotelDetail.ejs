<!--Checkbox filter script-->
<script>
    function generateCheckboxes(searchResult) {

        console.log(searchResult)
        if (searchResult.error === true) {
            console.log("In IF")
            Swal.fire({
                icon: "error",
                title: searchResult.message
            }).then(() => {
                window.location.href = "/hotels/hotelResults";
            })
        }

        const options = searchResult.recomData;

        // Extract unique meal types
        const mealTypes = new Set(options.map(option => option.StandardizedRooms[0].mappedRoomRates[0].boardBasis || "Unknown Meal"));

        // Generate checkboxes
        const filterContainer = document.getElementById("filter-container");
        // Add "Filter Rooms Accordingly" text
        filterContainer.innerHTML = `
    <div class="filter-header">
        <ul>
            <li><a href="#" style="font-size: 14px;" class="filter-link">Filter Rooms Accordingly: </a></li>
        </ul>
    </div>

    <label>
        <input type="checkbox" id="all" checked> All
    </label>
`;


        mealTypes.forEach(meal => {
            const checkboxHTML = `<label><input type="checkbox" class="meal-filter" value="${meal}"> ${meal}</label>`;
            filterContainer.innerHTML += checkboxHTML;
        });

        // Add event listeners to checkboxes
        document.getElementById("all").addEventListener("change", () => {
            if (document.getElementById("all").checked) {
                document.querySelectorAll(".meal-filter").forEach(cb => cb.checked = false); // Uncheck all other filters
                renderHTML(searchResult); // Show all options
            }
        });

        document.querySelectorAll(".meal-filter").forEach(checkbox => {
            checkbox.addEventListener("change", () => {
                document.getElementById("all").checked = false; // Uncheck "All" if specific filters are selected
                filterByMeal(searchResult);
            });
        });
    }

    function filterByMeal(searchResult) {
        console.log("In Filter By Meal");
        console.log(searchResult);

        if (!searchResult || !searchResult.recomData) {
            console.error("Invalid searchResult structure");
            return;
        }

        const options = searchResult.recomData;

        // Get selected meal types
        const selectedMeals = Array.from(document.querySelectorAll(".meal-filter:checked")).map(cb => cb.value);

        // Filter options
        const filteredOptions = selectedMeals.length > 0
            ? options.filter(option =>
                option?.StandardizedRooms?.some(room =>
                    room.mappedRoomRates?.some(rate => selectedMeals.includes(rate.boardBasis))
                )
            )
            : options;

        // Update searchResult correctly
        const updatedSearchResult = {
            ...searchResult,
            recomData: filteredOptions // Update recomData instead of nested ops
        };

        console.log("Updated Search Result:");
        console.log(updatedSearchResult);

        // Call renderHTML with updated data
        renderHTML(updatedSearchResult);
    }
</script>

