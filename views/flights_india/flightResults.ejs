        /* Override Select2 container styling */
        .select2-container--default .select2-selection--single {
            height: 42px !important;
            border: 0.0625rem solid rgb(221, 221, 221) !important;
            padding: 4px !important;
            border-radius: 6px !important;
            width: 100% !important;
            font-size: small !important;
            background-color: white; /* Ensure white background */
        }

        /* Adjust the position of the dropdown arrow */
        .select2-container--default .select2-selection__arrow {
            height: 42px !important; /* Match height */
            right: 10px; /* Adjust right padding */
            font-size: small !important;
        }

        /* Style the dropdown options */
        .select2-dropdown {
            border: 0.0625rem solid rgb(221, 221, 221) !important;
            font-size: small !important;
        }

        /* Style the placeholder */
        .select2-selection__placeholder {
            color: #aaa;
        }

        /* Style the focused and selected items */
        .select2-results__option--highlighted,
        .select2-results__option--selected {
            background-color: rgb(221, 221, 221);
            color: black;
        }

        .hidden {
            display: none; /* Hides the element */
        }

        .tab-content {
            display: none; /* Hide all sections by default */
        }

        .tab-content.active {
            display: block; /* Show only the active section */
        }

        /* CSS */
        .show {
            display: block !important;
        }

        .hide {
            display: none !important;
        }


        @import url("https://fonts.googleapis.com/css?family=Josefin+Sans:100,300,400,600,700");
        @import url("https://fonts.googleapis.com/css?family=Indie+Flower");


        .irs--round .irs-bar{
