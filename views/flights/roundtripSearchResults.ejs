    function isMorning(departureTime) {
        const hours = parseInt(departureTime.split(':')[0], 10);
        return hours >= 6 && hours < 12;
    }

    function isMidDay(departureTime) {
        const hours = parseInt(departureTime.split(':')[0], 10);
        return hours >= 12 && hours < 18;
